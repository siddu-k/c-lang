//What will be the output of the code snippet?

#include <stdio.h>
int main() {
    printf("Hello, ");
    printf("world!");
    return 0;
}

//What will be the output of the code snippet?

#include <stdio.h>
int main() {
    printf("%d ",10/2);
    printf("%d", 10/2.00 + 1);
    return 0;
}

//What would be the output of the following code:

#include <stdio.h>

int main() {
    char ch = 'A';
    printf("%c", ch + 1);
    return 0;
}

//What would be the output of the following code:

#include <stdio.h>

int main() {
    printf("%f", 10);
    return 0;
}


//Basic Expression Printing Test
#include <stdio.h>

int main() {
    printf("1 + 2 = %d", 1 + 2);
    return 0;
}
